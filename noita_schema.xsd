<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="AIAttackComponent">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:string">
					<xs:attribute name="frames_between" type="xs:int" use="optional" default="180">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">The minimum number of frames we wait between these attacks</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="frames_between_global" type="xs:int" use="optional" default="30">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">The minimum number of frames we wait after this attack before doing any other ranged attack</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="animation_name" use="optional" default="attack_ranged">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">The animation to play when performing this attack</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="attack_bullet"/>
								<xs:enumeration value="attack_grenade"/>
								<xs:enumeration value="attack_ranged"/>
								<xs:enumeration value="attack_ranged_fast"/>
								<xs:enumeration value="attack_stone"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="attack_ranged_entity_file" type="xs:string" use="optional" default="data/entities/projectiles/spear.xml">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">File to projectile entity that is created when performing a ranged attack</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_ranged_action_frame" type="xs:int" use="optional" default="2">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">The frame of the 'attack_ranged' animation during which the ranged attack actually occurs</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="min_distance" type="xs:int" use="optional" default="10">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">The minimum distance from enemy at which we can perform this attack.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="max_distance" type="xs:int" use="optional" default="160">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">The maximum distance from enemy at which we can perform this attack.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_ranged_offset_y" type="xs:int" use="optional" default="0">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">'attack_ranged_entity_file' is created here when performing a ranged attack</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="state_duration_frames" type="xs:int" use="optional" default="45">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">How long do we stay in the attack state, before other states are allowed?</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_ranged_offset_x" type="xs:int" use="optional" default="0">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">'attack_ranged_entity_file' is created here when performing a ranged attack</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="angular_range_deg" type="xs:int" use="optional" default="90">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">When looking for threats/prey this is our field of view around the X axis. 90 means we scan the whole 180 degrees around the X axis, to the left and right.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_ranged_entity_count_min" type="xs:int" use="optional" default="1">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">Minimum number of projectiles shot when performing a ranged attack</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_ranged_entity_count_max" type="xs:int" use="optional" default="1">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">Maximum number of projectiles shot when performing a ranged attack</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="use_probability" type="xs:int" use="optional" default="100">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">The probability for using this attack if it's otherwise possible</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_ranged_aim_rotation_enabled" type="xs:int" use="optional" default="0">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">If 1, we use a laser sight</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_ranged_aim_rotation_speed" type="xs:double" use="optional" default="3">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">How fast can we rotate our aim to track targets</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_ranged_aim_rotation_shooting_ok_angle_deg" type="xs:int" use="optional" default="10">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">If our aim is closer than this to the target we shoot</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_ranged_root_offset_y" type="xs:int" use="optional" default="0"/>
					<xs:attribute name="attack_ranged_use_laser_sight" type="xs:int" use="optional" default="0">
						<xs:annotation>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">If 1, we draw a laser sight to our target. Requires entity to have a sprite with tag 'laser_sight'</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_landing_ranged_enabled" use="optional" default="0">
						<xs:annotation>
							<xs:appinfo source="Auto-generated comment from missing attributes check" xml:lang="eng">This attribute was not found during schema auto-generation using all xml files. Unused in data.wak or at all?</xs:appinfo>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">If 1, we try to land before doing the attack, if there's ground near nearby under us</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_ranged_root_offset_x" use="optional" default="0">
						<xs:annotation>
							<xs:appinfo source="Auto-generated comment from missing attributes check" xml:lang="eng">This attribute was not found during schema auto-generation using all xml files. Unused in data.wak or at all?</xs:appinfo>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_ranged_use_message" use="optional" default="0">
						<xs:annotation>
							<xs:appinfo source="Auto-generated comment from missing attributes check" xml:lang="eng">This attribute was not found during schema auto-generation using all xml files. Unused in data.wak or at all?</xs:appinfo>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">If 1, we do ranged attacks by sending a Message_UseItem</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="attack_ranged_predict" use="optional" default="0">
						<xs:annotation>
							<xs:appinfo source="Auto-generated comment from missing attributes check" xml:lang="eng">This attribute was not found during schema auto-generation using all xml files. Unused in data.wak or at all?</xs:appinfo>
							<xs:documentation source="component_documentation.txt from build April 23 2021 (automated)" xml:lang="eng">If 1, we attempt to predict target movement and shoot accordingly</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
